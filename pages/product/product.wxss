@import "/utils/wxParse/wxParse.wxss";
@import "/pages/gallery/gallery.wxss";
.wxParse-img{
    width: 100%;
    display: block;
}
page{
    background: #efefef;
    padding-top: 0;
    padding-bottom: 45px;
}
.product-desc-loader{
    padding: 10px 0;
    margin-top: 10px;
    color: #999;
    text-align: center;
    font-size: 14px;
}
.product-desc-loader icon{
    margin-bottom: 5px;
    -webkit-transform: rotate(180deg);
            transform: rotate(180deg);
}
.product-actions{
    position: absolute;
    display: flex;
    align-items: center;
    right: 15px;
}
.product-actions .icon{
    width: 25px;
    height: 25px;
    vertical-align: inherit;
    background-color: rgba(255, 255, 255, 0.7);
    border-radius: 2px;
    background-size: 20px 20px;
}

.product-actions .action-item{
    margin-left: 10px;
}
.product-related .goods-list{
    border-top: none;
}
.tabbar-menu{
    overflow: hidden;
    height: 0;
}
.product-brand image{
    width: 50px;
    height: 50px;
}
.product-brand .weui-media-box{
    padding: 5px 15px;
}
.product-brand .weui-media-box__hd{
    margin-right: 10px;
}
.product-desc view{
    word-break:break-all;
    overflow:auto;
}

.product-desc .weui-form-preview:before,.product-desc .weui-form-preview:after{
    border: none;
}
.product-desc .weui-form-preview__hd{
    text-align: left;
    line-height: normal;
    font-size: 14px;
}
.product-desc .weui-form-preview__bd{
    line-height: normal;
    font-size: 12px;
}
.product-buycount{
    display: flex;
}
.slider{
    width: 100%;
    background: #fff;
    height: 300px;
}
.slide{
    width: 100%;
    height: 100%;
    display: block;
    margin: 0;
    padding: 0;
}
.product-name{
    display: flex;
    font-size: 14px;
    color: #333;
    justify-content:space-between;
}
.product-name .p-name{
    font-size: 16px;
    font-weight: 500;
}
.product-name .pn-left{

}
.defaultprice{
    background: #fff;
    display: flex;
    justify-content:space-between;
    align-items: center;
    padding: 10px 15px 10px 15px;
}
.price{
    color: #f00;
    font-size: 18px;
    font-stretch: ultra-condensed;
    font-family: Helvetica,sans-serif;
}
.mktprice{
  text-decoration: line-through;
  color:#999;
  margin-left:5px;
  font-family: Helvetica,sans-serif;
}
.product-container .weui-panel{
    margin-top:0;
}
.spec-select{
    font-size: 14px;
    margin-bottom: 10px;
}
.spec-title{
    min-width: 70px;
}
.spec-select,.spec-options{
    display: flex;

}
.spec-select{

}
.spec-options{
    flex-wrap: wrap;
    justify-content: flex-start;
}
.spec-item{
    border: 1px #ccc solid;
    margin: 0 5px 5px 0;
    padding: 3px 5px 3px 5px;
    display: flex;
    align-items: center;

}
.spec-item image{
    width: 40px;
    height: 40px;
}
.spec-item.active{
    border-color:#f60;
    color:#f60;
}
.spec-item.disabled{
    border-style: dashed;
    color: #ccc;
}
.spec-item.disabled image{
    opacity: 0.5;
}
.pb-label{
    font-size: 14px;
    width: 70px;
}
.fix-foot-bar{
    background: #efefef;
    height: 45px;
    width: 100%;
    position: fixed;
    display: flex;
    justify-content: space-between;
    bottom: 0;
    left: 0;
    z-index: 100;
}
.fix-foot-bar navigator{
        line-height: normal;
        padding: 0;
        margin: 0;
        height: 25px;
}
.foot-left{
    text-align:center;
    padding: 5px 10px 0 10px;

}
/* .foot-left contact-button{
    display: block;
} */
.product-online-comment::before,
.product-online-comment::after{
    border: none;
}
.product-online-comment{
    border: none;
    background: none;
    padding: 0;
    margin: 0;
    line-height: normal;
}
.icon-tip{
    font-size: 12px;
    color: #999;
    display: block;
    white-space: nowrap;
}
.foot-right{
    width: 100%;
    text-align: right;
}
.buy-area{
    display: flex;
    align-items: center;
    flex-direction:row-reverse;

}
.buy-area button{
    width: 50%;
    height: 45px;
    line-height: 45px;
    margin: 0;
    font-size: 14px;
    display: inline-block;
    border-radius: 0;
}
.buy-area button::before,.buy-area button::after{
    display: none;
    border-radius: 0;
}
.cart-quick-view{
    position: relative;
}
.fix-foot-bar .icon{
    height: 20px;
    width: 20px;
}
.cart-quick-view .cartcount{
    position: absolute;
    top:-8px;
    left:23px;
}

.gr-stars{
    display: flex;
    align-items: center;
}
.comment-hd{
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.comment-cell{
    margin-right: 4px;
}
.images-wrap{
    display: flex;
    justify-content: flex-start;
    align-content: flex-start;
    align-items: flex-start;
    flex-wrap: wrap;
}

.selected-image{
    padding: 1px;
    border: 1px #ccc solid;
    margin: 5px 5px 0 0;
}

.images-wrap.mini .selected-image{
    width: 40px;
    height: 40px;
}

.star-picker{
    display: flex;
    align-items: center;
}
.star-picker .star{
    color: #ccc;

}

.star-picker text{
    font-size: 24px;
    display: none;
    text-shadow: 1px 0px 0px #FFF;
}
.star-picker.sp-mini .star{
    padding:0 1px;
}
.star-picker.sp-mini text{
    font-size: 12px;
}
.star-picker text:last-child{
    display: inline;
}
.star-picker .active{
    color: #ffc926;
}
.star-picker .active text:last-child{
    display: none;
}
.star-picker .active text:first-child{
    display: inline;
    text-shadow: 1px 0px 0px #B38600;
}
.comment-item{
    display: block;
}
.comment-author{
    font-size: 13px;
    display: flex;
    align-items: center;
}
.comment-author .author-avatar{
    width: 30px;
    height: 30px;
    border-radius: 100%;
    margin-right: 5px;
}
.comment-content{
    font-size: 13px;
    color: #777;
    padding: 5px 0;
}
.comment-time{
    font-size: 12px;
    color: #999;
}
.product-params{
    padding: 1px 0;
}
.poster-canvas-mask{
    position: fixed;
    left: 0;
    top:0;
    background: rgba(0, 0, 0, 0.5);
    z-index: 999;
    display: flex;
    align-items: center;
    justify-content: center;
}
.poster-canvas-mask .mask-top{
    position: fixed;
    text-align: center;
    width: 100%;
}
.poster-canvas-box{


}
.poster-canvas-box button{
    position: relative;
    bottom: -10px;
}
.poster-canvas{

}
.icon-star-off{
    background-image: url(data:image/svg+xml;base64,);
}
.icon-star-on{
    background-image: url(data:image/svg+xml;base64,);
}
.icon-cart-empty{
    background-image: url(data:image/svg+xml;base64,);
}
.icon-cart-fill{
    background-image: url(data:image/svg+xml;base64,);
}

/*限时降价活动*/
.adjustprice{
    background: #f32353;/*#db2147*/
    color: #fff;
    font-family: Helvetica,sans-serif;
    display: flex;
    justify-content: space-between;
}
.adjustprice .ap-plan{
    display: flex;
    align-items: center;
    position: relative;
}
.ap-plan .end-price{
    font-size: 25px;
    margin:5px 10px 5px 15px;
}
.ap-plan .begin-price-wrap{
    margin:5px 0;
}
.ap-plan .begin-price{
    font-size: 12px;
    text-decoration:line-through;
    color: #efefef;
    margin-left: 10px;
}
.adjustprice .begin-price-wrap::after{
    content: " ";
    border-top: 27px solid #f32353;
    border-bottom: 27px solid #f32353;
    position: absolute;
    left: 100%;
    top:0;
    width: 0;
    height: 0;
    border-right: 13px solid transparent;
}
.plan-name{
    font-size: 12px;
    background: #db2147;
    border-radius: 2px;
    padding: 2px 10px;
}
.adjustprice .countdown{
  font-size: 14px;
  background:#fced83;
  white-space: nowrap;
  text-align: center;
  width: 130px;
  padding: 5px 0;
}
.countdown-tip{
    color: #f61d4b;
}
.adjustprice .countdown .countdown-timer{
    display: flex;
    align-items: center;
    justify-content: center;
    color:#543411;
}
.countdown-timer text{
    background: #543411;
    border-radius: 3px;
    font-size: 13px;
    padding:1px 2px;
    color:#fff;
    margin:0 2px;
}
.merchant {
  line-height:50px;
  padding:5px 15px;
}
.merchant image{
  width: 50px;
  height: 50px;
  float:left;
  margin-right:5px;
}
.merchant .go_shop,.merchant .merchant_name{
  float:left;
}
.merchant .go_shop{
  /* width:60px; */
  height:20px;
  font-size:12px;
  line-height:20px;
  /* background:#ff0000; */
  text-align:center;
  margin-top:15px; 
  position:relative;
  margin-left:5px;
  border:1px solid #999;
  color:#999;
  padding:0 5px;
  float:right;
  border-radius:3px;
}
