page{
 background:#efefef;
}
.member-top{
    position: relative;
    color: #fff;
    margin-top: 0;
}
.weui-panel.member-top::before,.weui-panel.member-top::after{
    border-top: none;
    border-bottom:none;
}
.weui-panel.member-top .weui-media-box::before {
    border-top: none;
}
.weui-grids{
    border-left: none;
}
.avatar-bg{
    position: absolute;
    width: 100%;
    height: 100%;
    -webkit-filter: blur(14px); /* Chrome, Opera */
       -moz-filter: blur(14px);
        -ms-filter: blur(14px);
            filter: blur(14px);
}
.avatar-bg-nowx{
  position: absolute;
    width: 100%;
    height: 100%;
}
.weui-cell {
    font-size: 14px;
}
.avatar{
    border-radius: 100%;
    border: 1px #fff solid;
    box-shadow: 1px 1px 1px #000;
}
.text-right{
    text-align: right;
}
.member-benefit{
    display:flex;
    position: relative;
    align-items: center;
    flex-direction: row;
    justify-content: space-around;
    background: linear-gradient(rgba(0,0,0,0.1),rgba(0,0,0,0.3));
    padding: 0;
    margin: 0;
    color:#fff;
    font-size: 12px;
}
.member-benefit navigator{
    width: auto;
    text-align: center;
    padding-top: 10px;
    width: 100%;
}
.member-benefit navigator .val{
    font-weight: lighter;
    line-height: 25px;
}
.member-benefit .weui-media-box::before{
    display: none;
}
.menu-order-groups{
    border-top: none;
    background: #fff;
}
.menu-order-groups .weui-grid__label {
    font-size: 12px;
}
.menu-order-groups .icon{
    display: block;
    margin: 0 auto;
}
.menu-order-groups .weui-grid{
    width: 20%;
    padding: 5px 5px;
}
.menu-order-groups .weui-grid .badge{
    color: #fff;
    background: rgba(255,0,0,0.7);
    border: 1px #fff solid;
    border-radius: 14px;
    font-size: 10px;
    position: absolute;
    top:5px;
    left: 60%;
    padding: 2px;
    min-width: 12px;
    min-height: 12px;
    text-align: center;
    line-height: 12px;
}
.menu-order-groups .weui-grid:last-child{
    border-right: none;
}
.levelname{
    font-size: 10px;
    margin-left: 10px;
    border: 1px #f90 solid;
    border-radius: 4px;
    color: #f90;
    padding: 0 3px;
}
.member-name{
    display: flex;
    align-items: center;
    justify-content: flex-start;
}

