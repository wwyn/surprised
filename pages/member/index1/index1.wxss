.weui-media-box__desc{
  color:#fff;
}

.noActivityData image{width:100px;display:block;margin:0 auto;margin-top:80px;}
.noActivityData .weui-msg{padding-top:10px;}
.noActivityData .weui-msg .weui-msg__title{font-size:14px;color:#888;}

/* 活动卡片 */
.has_radius{
	border-radius: 5px!important;
	    overflow: hidden;
}
.activecard{
    background-color: #fff;
     margin-bottom:15px;
}
.activecard.has_gap{
	margin: 15px 15px;
}
.activecard .content{
	padding:10px 15px;
}
.activecard .title{
	font-weight:bold;
	font-size:15px;
	line-height:25px;
	margin:0;
	overflow: hidden;
  white-space: nowrap;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;

}
.activecard .time,.activecard .address,.activecard .brief{
	line-height:22px;
	margin:0;
  font-size:12px;
	padding-left:20px;
	overflow: hidden;
  white-space: nowrap;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
  background:url('https://1716572wxaapp.vmcyun.com/public/images/68/69/17/0b2b943cb55ac38b6c6e6d63f63ab98e7de1dbb5.png?12217_OW27_OH27') no-repeat left center;
  background-size:16px 16px;
  color:#888;
}
.activecard .brief{
	background:none;
	padding-left:0px;
}
.activecard .address{
	background-image:url('https://1716572wxaapp.vmcyun.com/public/images/a5/70/a2/908f3dc52ed1acc49c8ab6b280dd9355af683ef8.png?12246_OW24_OH33');
	background-size:14px 17px;
}
.activecard .link{
    color: #000000;
    text-decoration: none;
}
.activecard .link:hover{
    text-decoration: none;
}

.activecard .image-block image{
  width:100%;
  display:block;
}

.activecard .activity_status{
  float:right;
  font-size:13px;
}
.activecard .activity_range{
  float:right;
  font-size:13px;
  color:#999;
  font-weight: normal;
}
.weui-loadmore{
  margin:0 auto;margin-top:45px;
}
.weui-loadmore__tips{
  width:30%;margin-bottom:35px;
}
.icon-time,.icon-location{
    width: 16px;
    height: 16px;
}