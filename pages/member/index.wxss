.show-details {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index:9999;
}

.details-box {
  width: 560rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 2rpx;
  padding: 36rpx;
  box-sizing: border-box;
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -285rpx;
  margin-left: -280rpx;
}

.details-title {
  font-size: 34rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.datails-content {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.datails-content text {
  color: #fc4773;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
}

.datails-content> view {
  text-indent: 2em;
  text-align: left;
}

.datails-btn {
  border: none;
  outline: none;
  width: 400rpx;
  height: 84rpx;
  background: rgba(252, 71, 115, 1);
  border-radius: 4px;
  text-align: center;
  line-height: 84rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
