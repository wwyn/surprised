.header {
  width: 100%;
  overflow: hidden;
  height: 300rpx;
  border-top: 1rpx solid #fc4773;
}

.page {
  width: 100%;
  overflow: hidden;
}

.member_info {
  width: 690rpx;
  height: 210rpx;
  background: #fff;
  margin-left: 30rpx;
  margin-top: 90rpx;
  border: 1rpx solid #fff;
  border-radius: 14rpx;
  box-shadow: 0rpx 4rpx 12rpx 2rpx RGBA(0, 0, 0, 0.09);
}

.member_avatar {
  position: absolute;
  left: 300rpx;
  top: 14rpx;
  width: 150rpx;
  height: 150rpx;
  border: 50%;
}

.member_avatar>image {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  border: none;
}

.member_nike {
  display: block;
  width: 100%;
  text-align: center;
  margin-top: 106rpx;
  font-family: PingFangSC;
  font-weight: Regular;
  color: #333;
  font-size: 28rpx;
  line-height: 40rpx;
}

.person_info {
  position: absolute;
  left: 594rpx;
  top: 118rpx;
  width: 130rpx;
  height: 36rpx;
}

.person_info>text {
  display: block;
  width: 88rpx;
  height: 30rpx;
  margin-left: 30rpx;
  margin-top: 4rpx;
  font-family: PingFangSC;
  font-weight: Regular;
  color: #fff;
  font-size: 22rpx;
  line-height: 30rpx;
}

.member_vip {
  margin-left: 246rpx;
}

.member_vip>image {
  width: 38rpx;
  height: 26rpx;
  margin-right: 8rpx;
}

.member_vip>.text1 {
  width: 154rpx;
  height: 38rpx;
  font-family: PingFangSC;
  font-weight: Regular;
  color: #f5a623;
  font-size: 26rpx;
  line-height: 38rpx;
}

.member_vip>.text2 {
  width: 154rpx;
  height: 38rpx;
  font-family: PingFangSC;
  font-weight: Regular;
  color: #f5a623;
  font-size: 22rpx;
  line-height: 38rpx;
}

/* 余额 */

.balance {
  width: 100%;
  overflow: hidden;
  height: 88rpx;
  border: 1rpx solid #fff;
  background-color: #fff;
  margin-top: 20rpx;
}

.balance_left {
  float: left;
  margin-left: 34rpx;
  margin-top: 24rpx;
}

.balance_left>.text1 {
  width: 56rpx;
  height: 40rpx;
  font-family: PingFangSC;
  font-weight: Regular;
  color: #333;
  font-size: 28rpx;
  line-height: 40rpx;
  margin-right: 10rpx;
}

.balance_left>.text2 {
  width: 120rpx;
  height: 40rpx;
  font-family: PingFangSC;
  font-weight: Regular;
  color: #fc4773;
  font-size: 28rpx;
  line-height: 40rpx;
}

.balance_recharge {
  float: right;
  margin-top: 30rpx;
  margin-right: 30rpx;
  width: 80rpx;
  height: 40rpx;
  border: 2rpx solid #fc4773;
  border-radius: 4rpx;
  font-family: PingFangSC;
  font-weight: Regular;
  color: #fc4773;
  font-size: 24rpx;
  line-height: 40rpx;
  text-align: center;
}

/* 销售业绩 */

.performance {
  width: 100%;
  overflow: hidden;
  height: 240rpx;
  background-color: #fff;
  margin-top: 20rpx;
}

.performance_top {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: 1rpx solid RGBA(235, 235, 235, 1);
}

.performance_top>text {
  margin-left: 34rpx;
  font-family: PingFangSC;
  font-weight: Medium;
  color: #333;
  font-size: 28rpx;
}

.performance_top image {
  display: inline-block;
  width: 52rpx;
  height: 24rpx;
  vertical-align: middle;
  margin-left: 10rpx;
  margin-bottom:4rpx;
}

.performance_detail {
  /* position: absolute;
  left: 648rpx;
  top: 438rpx; */
  float: right;
  margin-right: 30rpx;
}

.performance_detail>text {
  font-family: PingFangSC;
  font-weight: Medium;
  color: #999;
  font-size: 24rpx;
  line-height: 34rpx;
}

.performance_detail>image {
  width: 14rpx;
  height: 20rpx;
  border: none;
  margin-left: 10rpx;
}

.performance_main {
  width: 100%;
  height: 180rpx;
}

.performance_x {
  display: flex;
  justify-content: space-around;
}

.performance_today {
  display: flex;
  flex-direction: column;
  margin-top: 40rpx;
}

.performance_today>text {
  text-align: center;
}

.performance_today>.text1 {
  font-family: PingFangSC;
  font-weight: Regular;
  color: #fc4773;
  font-size: 28rpx;
  line-height: 40rpx;
}

.performance_today>.text2 {
  font-family: PingFangSC;
  font-weight: Regular;
  color: #666;
  font-size: 24rpx;
  line-height: 34rpx;
}

/* 我的订单 */

.my_order {
